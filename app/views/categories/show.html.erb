<h3>Category: <%= @category.title %></h3>
<%= link_to "Edit Category", edit_category_path(@category) %>
<br>
<br>
<ul>
    <% @category.jobs.each do |job| %>
        <li>
            <%= link_to job.title, company_job_path(job.company, job) %> at <%= job.company.name %>
        </li>
    <% end %>
</ul>
